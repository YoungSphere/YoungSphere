<h1>Upload#default</h1>
<div class="field">
    <%= form_with(model: @scenario, local: true) do |f| %>
        <%= render 'shared/error_messages' %>
        <%= f.label :title %>
        <%= f.text_field :title %>
        <%= f.file_field :static_website %>
        <%= f.submit "Upload", class: "btn btn-primary" %>
    <% end %>   
</div>
